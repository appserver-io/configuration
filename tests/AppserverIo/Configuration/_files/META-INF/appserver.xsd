<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:appserver="http://www.appserver.io/appserver"
           targetNamespace="http://www.appserver.io/appserver"
           elementFormDefault="unqualified"
           attributeFormDefault="unqualified">
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>

    <xs:complexType name="simpleElement">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="uuid" type="xs:string"/>
                <xs:attribute ref="xml:base"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:element name="access">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:params"/>
            </xs:choice>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="accesses">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:access"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="adapter-type">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="advice-aspect">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="advice-name">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="advice-pointcuts">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:pointcut-name"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="advice-type">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="advice">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:advice-aspect"/>
                <xs:element ref="appserver:advice-name"/>
                <xs:element ref="appserver:advice-type"/>
                <xs:element ref="appserver:advice-pointcuts"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="analytic">
        <xs:complexType>
            <xs:choice minOccurs="1" maxOccurs="1">
                <xs:element ref="appserver:connectors"/>
            </xs:choice>
            <xs:attribute name="uri" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="analytics">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:analytic"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="cron">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:jobs"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="jobs">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:job"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="job">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:schedule"/>
                <xs:element ref="appserver:execute"/>
            </xs:all>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="schedule">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string"/>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="execute">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:args"/>
            </xs:choice>
            <xs:attribute name="directory" type="xs:string" use="optional"/>
            <xs:attribute name="script" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="args">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:arg"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="arg">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="name" type="xs:string" use="optional"/>
                    <xs:attribute name="type" type="xs:string" use="required"/>
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="app">
        <xs:complexType>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="webappPath" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="apps">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:app"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="appserver">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:params"/>
                <xs:element ref="appserver:consoles" minOccurs="0"/>
                <xs:element ref="appserver:baseDirectory" minOccurs="0"/>
                <xs:element ref="appserver:initialContext" minOccurs="0"/>
                <xs:element ref="appserver:extractors" minOccurs="0"/>
                <xs:element ref="appserver:loggers" minOccurs="0"/>
                <xs:element ref="appserver:scanners" minOccurs="0"/>
                <xs:element ref="appserver:containers"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="consoles">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:console"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="console">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:params"/>
            </xs:choice>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="factory" type="xs:string" use="required"/>
            <xs:attribute name="createBackups" type="xs:boolean"/>
            <xs:attribute name="restoreBackups" type="xs:boolean"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="auth-type">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="auth">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:adapter-type"/>
                <xs:element ref="appserver:auth-type"/>
                <xs:element ref="appserver:options" minOccurs="0"/>
                <xs:element ref="appserver:realm" minOccurs="0"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="authentication">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:params"/>
            </xs:choice>
            <xs:attribute name="uri" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="authentications">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:authentication"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="baseDirectory">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="classLoader">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:params" minOccurs="0"/>
                <xs:element ref="appserver:directories"/>
                <xs:element ref="appserver:namespaces" minOccurs="0"/>
            </xs:all>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="interface" type="xs:string"/>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="factory" type="xs:string"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="classLoaders">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:classLoader"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueClassLoaderName">
            <xs:selector xpath="appserver:classLoader" />
            <xs:field xpath="@name" />
        </xs:unique>
    </xs:element>
    <xs:element name="connectionHandler">
        <xs:complexType>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="module">
        <xs:complexType>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="connectionHandlers">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:connectionHandler"/>
            </xs:choice>
        </xs:complexType>
        <xs:unique name="uniqueConnectionHandlerType">
            <xs:selector xpath="appserver:connectionHandler" />
            <xs:field xpath="@type" />
        </xs:unique>
    </xs:element>
    <xs:element name="connector">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="1">
                <xs:element ref="appserver:params"/>
            </xs:choice>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="connectors">
        <xs:complexType>
            <xs:choice minOccurs="1" maxOccurs="unbounded">
                <xs:element ref="appserver:connector"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueConnectorName">
            <xs:selector xpath="appserver:connector" />
            <xs:field xpath="@name" />
        </xs:unique>
    </xs:element>
    <xs:element name="container" >
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:description"/>
                <xs:element ref="appserver:upstreams" minOccurs="0"/>
                <xs:element ref="appserver:servers"/>
                <xs:element ref="appserver:deployment"/>
                <xs:element ref="appserver:host"/>
            </xs:all>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="factory" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="containers">
        <xs:complexType>
            <xs:choice minOccurs="1" maxOccurs="unbounded">
                <xs:element ref="appserver:container"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueContainerName">
            <xs:selector xpath="appserver:container" />
            <xs:field xpath="@name" />
        </xs:unique>
    </xs:element>
    <xs:element name="context-param">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:param-name"/>
                <xs:element ref="appserver:param-value"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="context">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:params"/>
                <xs:element ref="appserver:classLoaders"/>
                <xs:element ref="appserver:managers"/>
                <xs:element ref="appserver:provisioners"/>
            </xs:choice>
            <xs:attribute name="name" type="xs:string" use="required"/>
            <xs:attribute name="type" type="xs:string" use="optional"/>
            <xs:attribute name="factory" type="xs:string" use="optional"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="contexts">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:context"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueContextName">
            <xs:selector xpath="appserver:context" />
            <xs:field xpath="@name" />
        </xs:unique>
    </xs:element>
    <xs:element name="init-on-startup">
        <xs:complexType>
            <xs:simpleContent>
                <xs:restriction base="appserver:simpleElement">
                    <xs:enumeration value="true"/>
                    <xs:enumeration value="false"/>
                </xs:restriction>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="init-param">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:param-name"/>
                <xs:element ref="appserver:param-value"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="initialContext">
        <xs:complexType>
            <xs:choice maxOccurs="1">
                <xs:element ref="appserver:description" minOccurs="0"/>
                <xs:element ref="appserver:classLoader"/>
                <xs:element ref="appserver:storage"/>
            </xs:choice>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="injection-target-class">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="injection-target-method">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="injection-target-property">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="injection-target">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:injection-target-class"/>
                <xs:element ref="appserver:injection-target-method" minOccurs="0"/>
                <xs:element ref="appserver:injection-target-property" minOccurs="0"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="installation">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:steps" minOccurs="0"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="description">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="descriptor">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="descriptors">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="appserver:descriptor" maxOccurs="unbounded"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueDescriptor">
            <xs:selector xpath="appserver:descriptor" />
            <xs:field xpath="." />
        </xs:unique>
    </xs:element>
    <xs:element name="destination">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="directories">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:directory"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="directory">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="enforced" type="xs:string"/>
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="display-name">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="step">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="appserver:params" minOccurs="0" maxOccurs="1"/>
            </xs:choice>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="steps">
        <xs:complexType>
            <xs:choice minOccurs="0">
                <xs:element ref="appserver:step" maxOccurs="unbounded"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="storage">
        <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="appserver:servers"/>
            </xs:choice>
            <xs:attribute name="type" type="xs:string" use="required"/>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="servers">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:server"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueServerName">
            <xs:selector xpath="appserver:server" />
            <xs:field xpath="@name" />
        </xs:unique>
    </xs:element>
    <xs:element name="upstreams">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="appserver:upstream"/>
            </xs:choice>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
        <xs:unique name="uniqueUpstreamName">
            <xs:selector xpath="appserver:upstream" />
            <xs:field xpath="@name" />
        </xs:unique>
    </xs:element>
    <xs:element name="servlet-class">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="servlet-mapping">
        <xs:complexType>
            <xs:all>
                <xs:element ref="appserver:servlet-name"/>
                <xs:element ref="appserver:url-pattern"/>
            </xs:all>
            <xs:attribute name="uuid" type="xs:string"/>
            <xs:attribute ref="xml:base"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="servlet-name">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="uuid" type="xs:string"/>
                    <xs:attribute ref="xml:base"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
